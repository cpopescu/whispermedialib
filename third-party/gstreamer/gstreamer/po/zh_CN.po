#: gst/gst.c:307
msgid "Print the GStreamer version"
msgstr "打印 GStreamer 版本"

#: gst/gst.c:309
msgid "Make all warnings fatal"
msgstr "将所有的警告也当作严重问题"

#: gst/gst.c:313
msgid "Print available debug categories and exit"
msgstr "打印可用的调试类别并退出"

#: gst/gst.c:317
msgid ""
"Default debug level from 1 (only error) to 5 (anything) or 0 for no output"
msgstr "默认调试级别，从 1(仅有错误)到 5(所有内容)，0代表无输出"

#: gst/gst.c:319
msgid "LEVEL"
msgstr "级别"

#: gst/gst.c:321
msgid ""
"Comma-separated list of category_name:level pairs to set specific levels for "
"the individual categories. Example: GST_AUTOPLUG:5,GST_ELEMENT_*:3"
msgstr ""
"逗号分隔的“类别名称:级别”列表用来为每个类别设置指定的调试级别。如:"
"GST_AUTOPLUG:5,GST_ELEMENT_*:3"

#: gst/gst.c:324
msgid "LIST"
msgstr "列表"

#: gst/gst.c:326
msgid "Disable colored debugging output"
msgstr "禁止彩色的调试信息输出"

#: gst/gst.c:329
msgid "Disable debugging"
msgstr "禁止调试"

#: gst/gst.c:333
msgid "Enable verbose plugin loading diagnostics"
msgstr "开启详细的插件载入分析"

#: gst/gst.c:337
msgid "Colon-separated paths containing plugins"
msgstr "冒号分隔的包含插件的路径"

#: gst/gst.c:337
msgid "PATHS"
msgstr "路径"

#: gst/gst.c:340
msgid ""
"Comma-separated list of plugins to preload in addition to the list stored in "
"environment variable GST_PLUGIN_PATH"
msgstr ""
"逗号分隔的预载入的插件列表，而且此列表被保存于环境变量 GST_PLUGIN_PATH 中"

#: gst/gst.c:342
msgid "PLUGINS"
msgstr "插件"

#: gst/gst.c:345
msgid "Disable trapping of segmentation faults during plugin loading"
msgstr "禁止跟踪装入插件时的段严重错误"

#: gst/gst.c:350
msgid "Disable updating the registry"
msgstr "禁止更新寄存器"

#: gst/gst.c:355
msgid "Disable the use of fork() while scanning the registry"
msgstr "扫描寄存器时禁用 fork() 例程"

#: gst/gst.c:376
msgid "GStreamer Options"
msgstr "GStreamer 选项"

#: gst/gst.c:377
msgid "Show GStreamer Options"
msgstr "显示 GStreamer 选项"

#: gst/gst.c:752
#, c-format
msgid "Error writing registry cache to %s: %s"
msgstr "寄存器缓存写入 %s 时出错: %s"

#: gst/gst.c:788 gst/gst.c:797 gst/gst.c:842
#, c-format
msgid "Error re-scanning registry %s: %s"
msgstr "重新扫描寄存器 %s 时出错: %s"

#: gst/gst.c:857
#, c-format
msgid "Error re-scanning registry %s"
msgstr "重新扫描寄存器 %s 时出错"

#: gst/gst.c:1235
msgid "Unknown option"
msgstr "未知选项"

